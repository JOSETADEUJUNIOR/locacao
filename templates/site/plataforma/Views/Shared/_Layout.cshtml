    <script src="/App_Themes/plataforma/global/plugins/jquery-ui/jquery-ui-1.11.2.min.js"></script>
    <script src="/App_Themes/plataforma/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/js.cookie.min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="/App_Themes/plataforma/global/plugins/morris/morris.min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/morris/raphael-min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/counterup/jquery.waypoints.min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/counterup/jquery.counterup.min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/horizontal-timeline/horizontal-timeline.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/flot/jquery.flot.min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/flot/jquery.flot.resize.min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/flot/jquery.flot.categories.min.js" type="text/javascript"></script>
    <script src="/App_Themes/plataforma/global/plugins/jquery.sparkline.min.js" type="text/javascript"></script>

    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    <script src="/App_Themes/plataforma/global/scripts/app.min.js" type="text/javascript"></script>
    <!-- END THEME GLOBAL SCRIPTS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="/App_Themes/plataforma/pages/scripts/dashboard.min.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL SCRIPTS -->
    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    <script src="/App_Themes/plataforma/layouts/layout3/scripts/layout.min.js" type="text/javascript"></script>
    <!-- END THEME LAYOUT SCRIPTS -->
    @* Begin - Scripts da Manager *@
    <script src="~/Scripts/custom.js"></script>
    <script src="~/Scripts/jquery.mask.min.js"></script>
    <script src="/app_themes/plataforma/global/plugins/ckeditor/ckeditor.js" type="text/javascript"></script>
    @* End   - Scripts da Manager*@

    @* Begin - Scripts da Index *@
    <script src="/app_themes/plataforma/global/scripts/datatable.js" type="text/javascript"></script>
    <script src="/app_themes/plataforma/global/plugins/datatables/datatables.min.js" type="text/javascript"></script>
    <script src="/app_themes/plataforma/global/plugins/datatables/plugins/bootstrap/datatables.bootstrap.js" type="text/javascript"></script>
    <script src="/app_themes/plataforma/pages/scripts/table-datatables-responsive.js" type="text/javascript"></script>
    @* End   - Scripts da Manager *@

    @* Begin - Scripts da Index *@
    <link href="~/Scripts/datepicker.css" rel="stylesheet">
    <script src="~/Scripts/datepicker.js"></script>
    @* End    - Scripts da Index *@

    <script>
        $(document).ready(function () {

            $("#Data").mask('00/00/0000');
            $("#DataFim").mask('00/00/0000');
            $("#DataInicio").mask('00/00/0000');
            $("#DataExpiracao").mask('00/00/0000');
            $("#Data_Vencimento").mask('00/00/0000');
            $(".dental #CPFCNPJ").mask('00.000.000-0000-00');
            $(".cliente #CPFCNPJ").mask('000.000.000-00');
            $("#TelefoneCelular").mask('(00) 00000-0000');

        });
    </script>

    @RenderSection("scripts", required: false)

</body>
</html>
