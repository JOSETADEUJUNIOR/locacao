<head>
    <meta charset="utf-8" />
    <title>LDNA S/A - Plataforma</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="Codie" name="author" />

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    <link href="/App_Themes/plataforma/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="/App_Themes/plataforma/global/plugins/simple-line-icons/simple-line-icons.min.css" rel="stylesheet" type="text/css" />
    <link href="/App_Themes/plataforma/global/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />

    <link href="/App_Themes/plataforma/global/css/components.min.css" rel="stylesheet" id="style_components" type="text/css" />
    <link href="/App_Themes/plataforma/global/css/plugins.min.css" rel="stylesheet" type="text/css" />

    <link href="/App_Themes/plataforma/layouts/layout3/css/layout.min.css" rel="stylesheet" type="text/css" />
    <link href="/App_Themes/plataforma/layouts/layout3/css/themes/default.min.css" rel="stylesheet" type="text/css" id="style_color" />
    <link href="/App_Themes/plataforma/layouts/layout3/css/custom.min.css" rel="stylesheet" type="text/css" />

    @RenderSection("styles", required: false)

    <style>
        .ui-dialog-titlebar-close {
            height: 20px;
            line-height: 17px;
            border: 0;
            margin: 0;
            color: red;
        }

        .ui-dialog-title {
            display: none;
        }

        .ui-dialog-titlebar-close:before {
            content: "X";
        }
    </style>

    <link rel="shortcut icon" href="favicon.ico" />
</head>
<body class="page-container-bg-solid">
    <div class="page-wrapper">
        <div class="page-wrapper-row">
            <div class="page-wrapper-top">
                <div class="page-header">
                    <div class="page-header-top">
                        <div class="container-fluid">
                            <div class="page-logo">
                                <a href="/">
                                    <img class="login-logo" src="https://londrinasa.com.br/content/img/logo-londrinasa.png" style="margin-top:10px;" height="50">
                                </a>
                            </div>
                            <a href="javascript:;" class="menu-toggler"></a>
                            <div class="top-menu">
                                <ul class="nav navbar-nav pull-right">
                                    <li class="dropdown dropdown-extended quick-sidebar-toggler">
                                        <a href="@Url.Action("LogOff", "Login", new { area = "" })" style="padding:0;">
                                            <span class="sr-only">Sair</span>
                                            <i class="icon-logout"></i> SAIR
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="page-header-menu">
                        <div class="container-fluid">
                            <div class="hor-menu">
                                <ul class="nav navbar-nav">


                                    <li class="menu-dropdown classic-menu-dropdown">
                                        <a href="@Url.RouteUrl("cms.banners")">
                                            Banners
                                            <span class="arrow"></span>
                                        </a>
                                    </li>

                                    <li class="menu-dropdown classic-menu-dropdown">
                                        <a href="@Url.RouteUrl("catalogo.portfolios")">
                                            Produtos
                                            <span class="arrow"></span>
                                        </a>
                                    </li>

                                    <li class="menu-dropdown classic-menu-dropdown">
                                        <a href="@Url.RouteUrl("comunicacao.blogs")">
                                            Notícias
                                            <span class="arrow"></span>
                                        </a>
                                    </li>

                                    <li class="menu-dropdown classic-menu-dropdown">
                                        <a href="@Url.RouteUrl("propriedades.categorias")">
                                            Categorias
                                            <span class="arrow"></span>
                                        </a>
                                    </li>

                                </ul>
                            </div>
                            <!-- END MEGA MENU -->
                        </div>
                    </div>
                    <!-- END HEADER MENU -->
                </div>
            </div>
        </div>

        <div class="page-wrapper-row full-height">
            <div class="page-wrapper-middle">
                <div class="page-container">
                    <div class="page-content-wrapper">
                        <div class="page-head">
                            <div class="container-fluid">
                                <div class="page-title">
                                    <h1>
                                        @ViewBag.Title
                                    </h1>
                                </div>
                            </div>
                        </div>
                        <div class="page-content">
                            <div class="container-fluid">
                                <div class="page-content-inner">
                                    @RenderBody()
                                </div>
                            </div>
                        </div>
                    </div>
                    <a href="javascript:;" class="page-quick-sidebar-toggler">
                        <i class="icon-login"></i>
                    </a>

                </div>
                <!-- END CONTAINER -->
            </div>
        </div>


        <div class="page-wrapper-row">
            <div class="page-wrapper-bottom">
                <div class="page-footer">
                    <div class="container-fluid">
                        2009 - @DateTime.Now.Year © Criado e Desenvolvido por
                        <a target="_blank" href="https://londrinasa.com.br" rel="nofollow">Londrina S/A - Agência Digital</a> &nbsp;|&nbsp;
                        Suporte via telefone (43) 3338-2690 - Falar com Vinícius
                    </div>
                </div>
                <div class="scroll-to-top">
                    <i class="icon-arrow-up"></i>
                </div>
            </div>
        </div>
    </div>
    <!-- END QUICK NAV -->
    <!--[if lt IE 9]>
    <script src="/App_Themes/plataforma/global/plugins/respond.min.js"></script>
    <script src="/App_Themes/plataforma/global/plugins/excanvas.min.js"></script>
